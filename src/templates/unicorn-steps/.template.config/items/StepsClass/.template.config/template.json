{
    "$schema": "http://json.schemastore.org/template",
    "author": "Vitalii Dobriian",
    "classifications": ["Testing", "Unicorn.TAF"],
    "identity": "UnicornStepsTemplate.CSharp",
    "name": "Unicorn Test steps class",
